@model List<CalculatorApp.Models.Calculator>

@{
    ViewData["Title"] = "History";
}

<h2>History</h2>

@foreach (CalculatorApp.Models.Calculator item in Model)
{
    <div class="container">

        
            <div class="col-sm-1">
                 @item.LeftOperand
            </div>
        

        <div class="col-sm-1">
            @item.Operator
        </div>
    
        <div class="col-sm-1">
            @item.RightOperand
        </div>
    


        <div class="col-sm-1 ">
            <p>=</p>
        </div>


        <div class="col-sm-1">
            @item.Result
        </div>
   
    </div>

}